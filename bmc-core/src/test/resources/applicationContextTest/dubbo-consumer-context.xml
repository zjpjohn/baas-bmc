<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	<bean id="dubboConfig"
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="order" value="1" />
		<property name="ignoreUnresolvablePlaceholders" value="true" />
		<property name="systemPropertiesModeName" value="SYSTEM_PROPERTIES_MODE_OVERRIDE" />
		<property name="locations"
			value="classpath:applicationContextTest/dubbo.properties" />
	</bean>
	<!-- 消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
	<dubbo:application name="consumer-of-baas-bmc-test" />
	<dubbo:registry protocol="${dubbo.registry.protocol}"
		address="${dubbo.registry.address}" file="${dubbo.registry.file}" />
	<dubbo:consumer check="false" />
	<dubbo:reference id="iPricemakingSV"
		interface="com.ai.baas.bmc.api.pricemaking.interfaces.IPricemakingSV" />
	<dubbo:reference id="IAcctInfoSV"
		interface="com.ai.baas.bmc.api.acctinfo.interfaces.IAcctInfoSV" />
	<dubbo:reference id="iOrderInfoSV"
		interface="com.ai.baas.bmc.api.orderinfo.interfaces.IOrderInfoSV"></dubbo:reference>

</beans>